{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "cli": {
        "analytics": "68a952b6-061b-4e33-a09f-12cc9a4d3085"
    },
    "defaultProject": "guardian",
    "newProjectRoot": "projects",
    "projects": {
        "guardian": {
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "configurations": {
                        "development": {
                            "buildOptimizer": false,
                            "extractLicenses": false,
                            "namedChunks": true,
                            "optimization": false,
                            "sourceMap": true,
                            "vendorChunk": true
                        },
                        "production": {
                            "budgets": [
                                {
                                    "maximumError": "10mb",
                                    "maximumWarning": "5mb",
                                    "type": "initial"
                                },
                                {
                                    "maximumError": "10kb",
                                    "maximumWarning": "5kb",
                                    "type": "anyComponentStyle"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "outputHashing": "all"
                        }
                    },
                    "defaultConfiguration": "production",
                    "options": {
                        "allowedCommonJsDependencies": ["interfaces"],
                        "assets": ["src/favicon.ico", "src/assets"],
                        "index": "src/index.html",
                        "inlineStyleLanguage": "scss",
                        "main": "src/main.ts",
                        "outputPath": "dist/guardian",
                        "polyfills": "src/polyfills.ts",
                        "scripts": [],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                            "./node_modules/ngx-toastr/toastr.css",
                            "src/styles.scss"
                        ],
                        "tsConfig": "tsconfig.app.json"
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "guardian:build"
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "development": {
                            "browserTarget": "guardian:build:development",
                            "proxyConfig": "proxy.conf.json"
                        },
                        "production": {
                            "browserTarget": "guardian:build:production"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "assets": ["src/favicon.ico", "src/assets"],
                        "inlineStyleLanguage": "scss",
                        "karmaConfig": "karma.conf.js",
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "scripts": [],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                            "src/styles.scss"
                        ],
                        "tsConfig": "tsconfig.spec.json"
                    }
                }
            },
            "prefix": "app",
            "projectType": "application",
            "root": "",
            "schematics": {
                "@schematics/angular:application": {
                    "strict": true
                },
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "sourceRoot": "src"
        }
    },
    "version": 1
}
